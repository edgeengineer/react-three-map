{
  "name": "@edgeengineer/react-three-map",
  "version": "1.0.1",
  "description": "A React wrapper library for Google Maps 3D with React Three Fiber integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "repository": "https://github.com/edgeengineer/react-three-map",
  "homepage": "https://github.com/edgeengineer/react-three-map",
  "bugs": {
    "url": "https://github.com/edgeengineer/react-three-map/issues"
  },
  "license": "MIT",
  "author": "EdgeEngineer",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "prebuild": "tsc --project tsconfig.mapbox.json && tsc --project tsconfig.maplibre.json",
    "build": "yarn build:maplibre && yarn build:mapbox",
    "build:maplibre": "cross-env LIB_MODE=1 MAP_MODE=0 vite build",
    "postbuild:maplibre": "cross-env LIB_MODE=2 MAP_MODE=0 vite build",
    "build:mapbox": "cross-env LIB_MODE=1 MAP_MODE=1 vite build",
    "postbuild:mapbox": "cross-env LIB_MODE=2 MAP_MODE=1 vite build",
    "release": "npm run build && npm run changeset:publish",
    "changeset:publish": "changeset publish",
    "lint": "eslint src example-maplibre/src example-mapbox/src stories --ext ts,tsx --fix",
    "preview": "vite preview",
    "ts": "tsc -w",
    "ts:check": "tsc",
    "dev": "npm run storybook",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "preview:storybook": "npx serve storybook-static",
    "test": "npm run vitest",
    "vitest": "vitest"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@chromatic-com/storybook": "^1.9.0",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@react-three/postprocessing": "^2.19.1",
    "@storybook/addon-actions": "^8.5.0",
    "@storybook/addon-controls": "^8.5.0",
    "@storybook/addon-essentials": "^8.5.0",
    "@storybook/addon-links": "^8.5.0",
    "@storybook/blocks": "^8.5.0",
    "@storybook/react": "^8.5.0",
    "@storybook/react-vite": "^8.5.0",
    "@storybook/test": "^8.5.0",
    "@thatopen/components": "^3.1.1",
    "@thatopen/components-front": "^3.1.3",
    "@types/luxon": "^3.3.1",
    "@types/node": "^20.19.10",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/suncalc": "^1.9.0",
    "@types/three": "^0.175.0",
    "@types/tz-lookup": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react": "^4.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "glob": "^11.0.3",
    "happy-dom": "^10.5.2",
    "leva": "^0.9.35",
    "luxon": "^3.3.0",
    "mapbox-gl": "^3.9.4",
    "maplibre-gl": "^5.6.0",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "^8.0.1",
    "storybook": "^8.5.0",
    "suncalc": "^1.9.0",
    "suspend-react": "^0.1.3",
    "three": "^0.175.0",
    "three-stdlib": "^2.28.7",
    "typescript": "~5.0.2",
    "tz-lookup": "^6.1.25",
    "vite": "^4.5.14",
    "vitest": "^0.33.0",
    "web-ifc": "^0.0.70"
  },
  "peerDependencies": {
    "@react-three/fiber": ">=8.13",
    "mapbox-gl": ">=3.5.0",
    "maplibre-gl": ">=4.0.0",
    "react": ">=18.0",
    "react-map-gl": ">=8.0.0",
    "three": ">=0.133"
  },
  "peerDependenciesMeta": {
    "mapbox-gl": {
      "optional": true
    },
    "maplibre-gl": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-menubar": "^1.1.16",
    "lucide-react": "^0.539.0"
  }
}
